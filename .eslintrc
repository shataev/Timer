{
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "tsconfig.json",
    "sourceType": "module",
    "ecmaVersion": 2020
  },
  "extends": ["airbnb", "airbnb/hooks", "airbnb/whitespace", "airbnb-typescript"],
  "rules": {
    "import/prefer-default-export": "off",
    "react/function-component-definition": ["off"]
  }
}
